<DownPanel@TabbedPanel>:
    do_default_tab: False
    tab_pos: 'bottom_mid'
    font_size: 0.5, 1

    user_name: user_name
    search_input: search_input
    search_button: search_button

    TabbedPanelItem:
        text: "Лента"
        BoxLayout:
            orientation: 'vertical'
            MDScreen:

                MDSwiper:
                    # size: None
                    height: root.height  - dp(40)
                    y: root.height - self.height - dp(20)

                    SwiperOne:
                        FitImage:
                            source: 'libs/images/1.jpg'
                            radius: [20,]
                    SwiperOne:
                        FitImage:
                            source: 'libs/images/2.jpg'
                            radius: [20,]
                    SwiperOne:
                        FitImage:
                            source: 'libs/images/3.jpg'
                            radius: [20,]
                    SwiperOne:
                        FitImage:
                            source: 'libs/images/4.jpg'
                            radius: [20,]
                    SwiperOne:
                        FitImage:
                            source: 'libs/images/5.jpg'
                            radius: [20,]

            
